{
    "version": 2,
    "buildCommand": "sh build.sh",
    "outputDirectory": "public",
    "functions": {
        "api/index.js": {
            "runtime": "vercel-node@latest"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        }
    ]
}