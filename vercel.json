{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "vercel-node@latest"
        }
    },
    "routes": [
        {
            "src": "/api(/(.*))?",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}