{
    "version": 2,
    "buildCommand": "mkdir -p frontend/dist && cp frontend/index.html frontend/dist/index.html",
    "outputDirectory": "frontend/dist",
    "functions": {
        "api/index.js": {
            "runtime": "vercel-node@latest"
        }
    },
    "routes": [
        {
            "src": "/api(/(.*))?",
            "dest": "/api/index.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}